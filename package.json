{
  "name": "solid-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:https": "ng serve --host 0.0.0.0 --ssl --ssl-key ./certs/localhost.key  --ssl-cert ./certs/localhost.crt",
    "build": "ng build",
    "lint": "ng lint",
    "test": "ng test --code-coverage",
    "e2e": "ng e2e",
    "postinstall": "node patch.js",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "docs:prepare": "npm install -g gitbook-cli && gitbook install",
    "docs:watch": "gitbook serve ./a_docs ./docs/docs --open",
    "docs:build": "gitbook build ./a_docs ./docs/docs",
    "ghpages:build": "ng build --prod --base-href \"https://arquisoft.github.io/dechat_en2a/\" --output-path \"docs/\" && npm run docs:build"
  },
  "browser": {
    "crypto": false,
    "fetch": false,
    "window": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.1.4",
    "@ng-select/ng-select": "^2.5.1",
    "@solid/cli": "^0.1.0",
    "@solid/query-ldflex": "^1.1.1",
    "@types/chai": "^4.1.7",
    "@types/cucumber": "^4.0.5",
    "chai": "^4.2.0",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "cucumber": "^5.1.0",
    "date-fns": "^2.0.0-alpha.25",
    "http": "^0.0.0",
    "inquirer": "^6.2.0",
    "isomorphic-fetch": "^2.2.1",
    "n3": "^1.0.2",
    "ngx-toastr": "^9.0.2",
    "node-fetch": "^2.2.0",
    "path": "^0.12.7",
    "prefix-ns": "^0.1.2",
    "process": "^0.11.10",
    "protractor-cucumber-framework": "^6.1.1",
    "purecss": "^1.0.0",
    "q": "^1.5.1",
    "rdflib": "^0.19.0",
    "rxjs": "^6.0.0",
    "solid-auth-client": "^2.2.10",
    "solid-file-client": "^0.4.9",
    "stdio": "^0.2.7",
    "streamify-array": "^1.0.1",
    "text-encoding": "^0.6.4",
    "uniqid": "^5.0.3",
    "uri-js": "^4.2.2",
    "webcrypto": "^0.1.1",
    "whatwg-url": "^7.0.0",
    "winston": "^3.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "^6.2.3",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "coveralls": "^3.0.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
